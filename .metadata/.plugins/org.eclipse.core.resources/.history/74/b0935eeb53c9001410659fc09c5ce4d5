package nju.edu.cn;

public class QuickSort {
	
	public void quickSort(int[] array, int low, int high){
		int middle = array[low];
		
		while (low < high) {
			if (array[high] < middle) {
				array[low] = array[high];
			}
			high++;
		}
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
