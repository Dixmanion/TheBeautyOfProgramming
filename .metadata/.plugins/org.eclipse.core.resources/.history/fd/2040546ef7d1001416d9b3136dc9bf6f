package nju.edu.cn;

/**
 * 将两个有序表合并成一个有序表
 * @author eureka
 *
 */
public class MergeSort {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
	}
	
	//递归调用的方法
	public void mergeSort(int[] a, int start, int length) {
		
		
	}
	
	//将两个有序序列合并起来，可以参考两个有序链表的合并
	private void merge(int[] a, int start, int reStart, int length) {
		int low = start;
		int high = reStart;
		int[] temp = new int[reStart + length - start];
		int tempIndex = 0;
		
		while (low < reStart && high < (reStart+length)) {
			if (a[high] < a[low]) {
				temp[tempIndex] = a[high];
				tempIndex++;
				high++;
			} else if (a[low] < a[high]){
				temp[tempIndex] = a[low];
				low++;
				tempIndex++;
			} else if (a[low] == a[high]) {
				temp[tempIndex] = a[low];
				low++;
				tempIndex++;
				temp[tempIndex] = a[high];
				high++;
				tempIndex++;
			}
		}
		
		while (low < reStart) {
			temp[tempIndex] = a[low];
			low++;
			tempIndex++;
		}
	}

