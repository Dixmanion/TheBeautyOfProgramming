package nju.edu.cn;


public class Test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] num = {-1, -1, -1, -1, 0, 0, 0 ,1 ,1 ,1};
		int[] noDup = new int[num.length];
        int lastValue = num[0];
        int counter = 1;
        int index = 0; //noDupçš„index
        noDup[0] = lastValue;
        
        for (int i = 1; i < num.length; i++) {
            if (lastValue == num[i]) {
                counter++;
                if (counter == 2) {
                	index ++;
                    noDup[index] = num[i];
				}
            } 
            if (lastValue != num[i]){
                lastValue = num[i];
                counter = 1;
                index ++;
                noDup[index] = num[i];
            } 
        }
        
        for (int i = 0; i < index; i++) {
			System.out.println(noDup[i]);
		}
	}

}
